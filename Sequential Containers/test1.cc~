#include "ArrayIntList.h"
#include <iostream>

using namespace std;

int main()
{
   ArrayIntList<int> test;
   cout<<test.isEmpty()<<endl;
   cout << "Capacity" << charArray.getCapacity() << endl;
   for (int i=0;i<10;i++)
      charArray.pushBack(i);
   charArray.print();
   cout<<"pushBack"<< endl;
   charArray.pushBack(11);
   charArray.print();
   cout<<"popBack"<<endl;
   charArray.popBack(); 
   charArray.print();
   cout << "getBack" << charArray.getBack() << endl;
   charArray.pushFront(12);
   charArray.pushFront(29);
   charArray.print();
   cout << "getFront " << charArray.getFront() << endl;
   charArray.popFront();
   cout<<"popFronted"<<endl;
   charArray.print();
   cout << "getItem 5" << charArray.getItem(5) << endl;
   charArray.remove(3);
   charArray.print();
   charArray.setItem(3,1000);
   cout << "Get Item " << charArray.getItem(3) << endl;
   cout << "getBack " << charArray.getBack() << endl;
   cout << "getFront " << charArray.getFront() << endl;
   charArray.print();
   for (int i = charArray.getSize();i>0;i--)
   {
      charArray.popBack();
      cout<<charArray.getBack()<<"back"<<endl;
   }
   cout<<"Empty ="<<charArray.isEmpty()<<endl;
   cout<<"insert CharArray:"<<endl;
   for (int i=97;i<110;i++)
   {
      charArray.insert(i,char(i));
      cout<<charArray.getBack()<<endl;
   }
   charArray.print();
   for (int i=14;i>-1;i--)
   {
      charArray.remove(i);
      cout<<charArray.getBack()<<endl;
   }
   charArray.print();	
}

